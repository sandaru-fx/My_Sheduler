    "express": "^5.2.1",
    "lucide-react": "^0.460.0",
    "mongoose": "^9.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "three": "^0.170.0",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
